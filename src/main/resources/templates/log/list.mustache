{{>layout/header}}
        <div class="container text-white mt-5 p-5 rounded">
            <h1 class="text-dark">구매목록</h1>
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th>주문번호</th>
                        <th>구매자</th>
                        <th>상품명</th>
                        <th>구매 수량</th>
                        <th>총 가격</th>
                    </tr>
                </thead>
                <tbody>
                    {{#models}}
                    <tr>
                        <td>{{id}}</td>
                        <td>{{buyer}}</td>
                        <td>{{name}}</td>
                        <td>{{qty}}개</td>
                        <td>{{totalPrice}}원</td>
                    </tr>
                    {{/models}}
                </tbody>
            </table>
        </div>
{{>layout/footer}}
