{{>layout/header}}
        <div class="container text-white mt-5 p-5 rounded">
            <h1 class="text-dark">상품목록</h1>
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th>번호</th>
                        <th>상품명</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                {{#models}}
                    <tr>
                        <td>{{id}}</td>
                        <td>{{name}}</td>
                        <td><a href="/store/{{id}}">상세보기</a></td>
                    </tr>
                {{/models}}
                </tbody>
            </table>
        </div>
{{>layout/footer}}